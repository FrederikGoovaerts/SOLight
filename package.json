{
  "name": "solight",
  "version": "0.1.0",
  "description": "Stackoverflow Light Backend",
  "author": "Frederik Goovaerts",
  "private": "true",
  "scripts": {
    "start": "ts-node src/main.ts",
    "test": "npm run typecheck && npm run lint && npm run prettier",
