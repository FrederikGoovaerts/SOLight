{
  "name": "solight",
  "version": "0.1.0",
  "description": "Stackoverflow Light Backend",
  "author": "Frederik Goovaerts",
  "private": "true",
  "scripts": {
    "start": "ts-node src/main.ts",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "test": "npm run typecheck && npm run unit-test && npm run lint && npm run prettier",
    "unit-test": "jest",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrederikGoovaerts/SOLight.git"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-openid-connect": "^2.3.0",
    "pg": "^8.5.1",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  }
}
